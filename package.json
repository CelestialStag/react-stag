{
	"name": "react-stag",
	"version": "1.0.0",
	"description": "",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"lint": "eslint ./src/ --ext .ts,.tsx,.json",
		"pretty": "eslint ./src/ --ext .ts,.tsx,.json --quiet --fix",
		"type-check": "tsc --noEmit",
		"type-check:watch": "npm run type-check -- --watch",
		"watch": "nodemon --watch './src/**/*' -e ts,tsx --exec yarn build",
		"build": "npm run build:types && npm run build:js",
		"build:types": "tsc",
		"build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/songmawa/react-stag.git"
	},
	"keywords": [],
	"author": "",
	"license": "GPL-3.0",
	"bugs": {
		"url": "https://github.com/songmawa/react-stag/issues"
	},
	"homepage": "https://github.com/songmawa/react-stag#readme",
	"dependencies": {
		"prettier": "^2.2.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2"
	},
	"devDependencies": {
		"@babel/cli": "^7.13.16",
		"@babel/core": "^7.13.16",
		"@babel/plugin-proposal-class-properties": "^7.13.0",
		"@babel/preset-env": "^7.13.15",
		"@babel/preset-react": "^7.13.13",
		"@babel/preset-typescript": "^7.13.0",
		"@types/node": "^14.14.41",
		"@types/react": "^17.0.3",
		"@types/react-dom": "^17.0.3",
		"@typescript-eslint/eslint-plugin": "^4.22.0",
		"@typescript-eslint/parser": "^4.22.0",
		"babel-plugin-module-resolver": "^4.1.0",
		"babel-plugin-root-import": "^6.6.0",
		"eslint": "^7.25.0",
		"eslint-config-prettier": "^8.2.0",
		"eslint-plugin-prettier": "^3.4.0",
		"eslint-plugin-react": "^7.23.2",
		"nodemon": "^2.0.7",
		"ts-loader": "^8.2.0",
		"tsconfig-paths-webpack-plugin": "^3.5.1",
		"typescript": "^4.2.4"
	}
}
